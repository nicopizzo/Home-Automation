@page "/settings"

<div class="d-flex flex-column gap-2">
    <div class="d-flex gap-2">
        <span>Api Url</span>
        <InputText DisplayName="Api Url" @bind-Value="@_ApiUrl" />
    </div>
    <div class="d-flex gap-2">
        <span>Api Key</span>
        <InputText DisplayName="Api Key" @bind-Value="@_ApiKey" />
    </div>
    <button @onclick="() => OnSaveClick()">Save</button>
</div>
